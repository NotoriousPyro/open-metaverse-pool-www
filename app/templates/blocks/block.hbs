<tr>
    <!--<td><a href="http://mymvs.info/block/{{block.height}}" rel="nofollow" target="_blank">{{format-number block.height}}</a></td>-->
    <td><a href="https://explorer.mvs.org/#!/blk/{{block.height}}" rel="nofollow" target="_blank">{{format-number block.height}}</a></td>
    <td>{{format-date-locale block.timestamp}}</td>
    <td>
        {{#if block.isLucky}}
        <span class="label label-success">{{format-number block.variance}}&#37;</span>
        {{else}}
        <span class="label label-primary">{{format-number block.variance}}&#37;</span>
        {{/if}}
    </td>
    <td>
        {{#if block.orphan}}
        <span class="label label-default">{{block.formatReward}}</span>
        {{else if block.isOk}}
        <span class="label label-info">{{block.formatReward}}</span>
        {{/if}}
    </td>
    <td>
        {{#if block.orphan}}
        <span class="label label-danger">Orphan</span>
        {{else}}
        <!--<a href="http://mymvs.info/block/{{block.height}}" class="hash" rel="nofollow" target="_blank">{{block.hash}}</a>-->
        <a href="https://explorer.mvs.org/#!/blk/{{block.height}}" class="hash" rel="nofollow" target="_blank">{{block.hash}}</a>
        {{/if}}
    </td>
</tr>