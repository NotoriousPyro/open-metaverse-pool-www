{{#if model.immature}}
<div class="table-responsive">
  <table class="table table-condensed">
    <thead>
      <tr>
        <th>Height</th>
        <th>Time Found</th>
        <th>Variance</th>
        <th>Reward</th>
        <th style="width: 50%;">Block Hash</th>
      </tr>
    </thead>
    <tbody>
      {{#each model.immature as |block|}}
        {{partial "blocks/block"}}
      {{/each}}
    </tbody>
  </table>
</div>
{{else}}
<h3>No immature blocks yet</h3>
{{/if}}
